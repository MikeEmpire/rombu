        
  <table class="table table-striped">
    <thead>
      <tr>
        {{!-- <th>ID</th> --}}
        <th>Video Name</th>
        <th>Company</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
  	  {{#each data}}
      <tr>
        {{!-- <td>{{id}}</td> --}}
        <td>{{name}}</td>
        <td>{{company}}</td>
        <td>
          <button class="btn btn-default delete" data-id={{this.id}} type="submit">Delete</button>
        </td>
      </tr>
			{{/each}}
    </tbody>
  </table>